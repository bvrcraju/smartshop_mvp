{
  "name": "ecom-core",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "populate": "ts-node populate.ts",
    "compile-admin-ui": "ts-node src/compile-admin-ui.ts",
    "run:server": "ts-node ./src/index.ts",
    "run:worker": "ts-node ./src/index-worker.ts",
    "prod:server": "node ./dist/index.js",
    "prod:worker": "node ./dist/index-worker.js",
    "live": "concurrently yarn:prod:*",
    "pm-start": "pm2-runtime process.json",
    "start": "concurrently yarn:run:*",
    "build": "rimraf dist && rimraf admin-ui && tsc && tsc -p tsconfig.migrations.json && yarn compile-admin-ui",
    "generate-types": "graphql-codegen --config ./codegen.json",
    "migration:generate": "ts-node migration generate",
    "migration:run": "ts-node migration run",
    "migration:revert": "ts-node migration revert",
    "lint:fix": "eslint ./src/ --ext .ts --fix",
    "e2e:reviews": "jest --config src/plugins/reviews/e2e/config/jest-config.js --runInBand"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^7.16.0",
    "@vendure/admin-ui-plugin": "^1.4.3",
    "@vendure/asset-server-plugin": "^1.4.3",
    "@vendure/core": "^1.4.3",
    "@vendure/elasticsearch-plugin": "^1.4.3",
    "@vendure/email-plugin": "^1.4.3",
    "@vendure/ui-devkit": "^1.4.3",
    "aws-sdk": "^2.1037.0",
    "crypto": "^1.0.1",
    "dotenv": "^10.0.0",
    "firebase-admin": "^10.0.0",
    "hbs": "^4.2.0",
    "js-base64": "^3.7.2",
    "pg": "8.7.1"
  },
  "devDependencies": {
    "@graphql-codegen/add": "2.0.2",
    "@graphql-codegen/cli": "1.21.1",
    "@graphql-codegen/typescript": "1.21.0",
    "@graphql-codegen/typescript-compatibility": "2.0.1",
    "@graphql-codegen/typescript-operations": "1.17.14",
    "@types/geojson": "^7946.0.8",
    "@types/jest": "^27.0.2",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "@vendure/create": "^1.4.3",
    "@vendure/testing": "^1.4.3",
    "concurrently": "6.3.0",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.3.1",
    "prettier": "^2.4.1",
    "ts-jest": "^27.0.7",
    "ts-node": "10.4.0",
    "typescript": "4.3.5"
  }
}
